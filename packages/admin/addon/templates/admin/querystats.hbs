<div class="navi-querystats-dashboard__kpi">
  <div class="navi-querystats-dashboard__kpi-bar">
    {{!-- <BarGraphModel @model={{@model}} class="navi-querystats-dashboard__bar-style" /> --}}
    <ApexChart
    @type={{this.type}}
    @width={{this.width}}
    @height={{this.height}}
    @series={{this.series}}
    @chartOptions={{this.barOptions}}
    {{!-- @onMounted={{this.mountedHandler}} --}}
    @onBeforeMount={{this.beforeMountHandler}}
    />
  </div>

  <div class="navi-querystats-dashboard__kpi-donut">
    {{!-- <BarGraphModel @model={{@model}} class="navi-querystats-dashboard__bar-style" /> --}}
    <ApexChart
    @type={{this.type_2}}
    @width={{this.width_2}}
    @height={{this.height_2}}
    @series={{this.series_2}}
    @chartOptions={{this.donutOptions}}
    {{!-- @onMounted={{this.mountedHandler}} --}}
    @onBeforeMount={{this.beforeMountHandlerPie}}
    />
  </div>
</div>

<div class="navi-querystats-dashboard__timeseries">
  {{!-- <BarGraphModel @model={{@model}} class="navi-querystats-dashboard__bar-style" /> --}}
  <ApexChart
  {{!-- @type={{this.type_2}}
  @width={{this.width_2}}
  @height={{this.height_2}} --}}
  {{!-- @series={{this.series_3}} --}}
  @chartOptions={{this.chartOptions}}
  {{!-- @onMounted={{this.mountedHandler}} --}}
  @onBeforeMount={{this.beforeMountHandlerTime}}
  />
</div>


<div class="navi-querystats-dashboard__tablebalh">
  <QueryStatsTable @model={{@model}} class="navi-querystats-dashboard__table-style"/>
</div>

